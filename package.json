{
  "version": "0.0.4",
  "license": "AGPL-3.0-only",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --format umd",
    "build": "tsdx build --format umd",
    "test": "tsdx test",
    "posttest": "npm run format",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "serve": "serve ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "lunar_assembler",
  "author": "Mateusz Konieczny",
  "module": "dist/lunar_assembler.esm.js",
  "size-limit": [
    {
      "path": "dist/lunar_assembler.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/lunar_assembler.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/d3": "^5.0.0",
    "@types/leaflet": "^1.7.0",
    "@types/leaflet-draw": "^0.4.12",
    "husky": "^6.0.0",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "d3": "^6.6.2",
    "leaflet": "^1.7.1",
    "leaflet-draw": "github:Leaflet/Leaflet.draw"
  }
}
